getUserById:
SELECT * FROM User WHERE id = ?;

insertUser:
INSERT INTO User
(id, name, surname, email, phone, date, isAnonymous)
VALUES
(?, ?, ?, ?, ?, ?, ?);

updateUser:
UPDATE User
SET name = ?, surname = ?, phone = ?, date = ?, isAnonymous = ?
WHERE id = ?;

deleteUser:
DELETE FROM User
WHERE id = ?;

deleteAllUsers:
DELETE FROM User;
